{
  "compilerOptions": {
    "target": "ESNext",
  "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "jsx": "react-jsx",
    "allowJs": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
  "types": ["node", "react", "react-dom"],
    "allowSyntheticDefaultImports": true
  },
  "exclude": ["dist", "node_modules"]
}
