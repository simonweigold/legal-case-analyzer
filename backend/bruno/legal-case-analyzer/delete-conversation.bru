meta {
  name: delete-conversation
  type: http
  seq: 12
}

delete {
  url: http://localhost:8000/conversations/{{new_conversation_id}}
  body: none
  auth: bearer
}

auth:bearer {
  token: {{auth_token}}
}

headers {
  Content-Type: application/json
}

settings {
  encodeUrl: true
}

tests {
  test("should delete conversation", function() {
    expect(res.getStatus()).to.equal(200);
    expect(res.getBody()).to.have.property('message');
    expect(res.getBody().message).to.contain('deleted successfully');
  });
}
